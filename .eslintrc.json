{
    "extends": [
        "airbnb"
    ],
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],    
         "implicit-arrow-linebreak": "off",
        "comma-dangle": "off",
        "no-console": "off",
        "react/prop-types": 0,
        "no-underscore-dangle":  ["error", { "allow": ["_id"] }]
    }
}